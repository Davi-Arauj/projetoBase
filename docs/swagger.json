                    {
                        "type": "integer",
                        "description": "ID do usuario a ser removido",
                        "name": "usuario_id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "204": {
                        "description": "No Content",
                        "schema": {
                            "type": "nil"
                        }
                    }
                }
            }
