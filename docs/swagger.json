                    "cadastros/usuario"
                ],
                "summary": "Adiciona um novo usuario",
                "parameters": [
                    {
                        "description": "Dados do usuario",
                        "name": "tag",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/usuario.Req"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "id do novo usuario adicionado",
                        "schema": {
