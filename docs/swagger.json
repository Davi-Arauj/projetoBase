                        "schema": {
                            "$ref": "#/definitions/usuario.Req"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "No Content",
                        "schema": {
                            "type": "nil"
                        }
                    }
                }
            },
            "delete": {
                "description": "Remove um usuario",
