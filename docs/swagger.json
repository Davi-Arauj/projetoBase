            "put": {
                "description": "Altera um usuario dado o seu id e o corpo do novo usuario",
                "consumes": [
                    "application/json"
                ],
                "tags": [
                    "cadastros/usuario"
                ],
                "summary": "Altera um usuario",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "ID do usuario a ser alterado",
                        "name": "usuario_id",
                        "in": "path",
                        "required": true
                    },
                    {
                        "description": "Dados do novo usuario",
                        "name": "tag",
                        "in": "body",
